{
  "name": "@advancedalgos/financial-beings-client",
  "version": "0.0.4",
  "description": "Advanced Algos Financial beings client-side web app",
  "author": "",
  "license": "",
  "main": "src/index.js",
  "scripts": {
    "flow-coverage": "npm run flow-coverage:build && npm run flow-coverage:open",
    "flow-coverage:build": "flow-coverage-report  --strict-coverage ",
    "flow-coverage:open": "concurrently \"cd flow-coverage && http-server -c-1 \" \"open http://127.0.0.1:8080\"",
    "flow": "flow",
    "lint-fix": "eslint --fix src/"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "4.0.2",
    "react-hot-loader": "^4.3.11",
    "flow-bin": "0.79.1",
    "babel-preset-flow": "6.23.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "apollo-client": "^2.4.4",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "luxon": "^1.4.4",
    "material-ui-pickers": "^1.0.1",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-d3-tree": "^1.12.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.1",
    "react-sortable-hoc": "^0.8.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
